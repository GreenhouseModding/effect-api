{
    "schemaVersion": 1,
    "id": "${mod_id}_test",
    "version": "${version}",
    "name": "${mod_name} Test",
    "description": "${mod_description}",
    "authors": [
        "${mod_author}"
    ],
    "contributors": [
        ${fabric_mod_contributors}
    ],
    "contact": {
        "homepage": "${homepage}",
        "issues": "${issues}",
        "sources": "${sources}"
    },
    "license": "${mod_license}",
    "icon": "${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "dev.greenhouseteam.test.EffectAPITestFabric"
        ],
        "client": [
            "dev.greenhouseteam.test.client.EffectAPITestFabricClient"
        ],
        "preLaunch": [
            "dev.greenhouseteam.test.EffectAPITestFabricPre"
        ]
    },
    "depends": {
        "fabricloader": "${fabric_loader_range}",
        "fabric-api": "*",
        "minecraft": "${fabric_minecraft_version_range}",
        "java": ">=${java_version}",
        "effectapi_base": ">=${version}",
        "effectapi_entity": ">=${version}"
    }
}